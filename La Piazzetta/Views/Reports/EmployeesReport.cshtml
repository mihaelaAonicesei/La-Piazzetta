@model List<La_Piazzetta.Models.Employee>

<h2 style="font-family: 'Playfair Display', serif; font-weight: 600">Employees Report</h2>

<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Position</th>
            <th>Phone Number</th>
            <th>Employment start date</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var em in Model)
        {
            <tr>
                <td>@em.FirstName</td>
                <td>@em.LastName</td>
                <td>@em.Position</td>
                <td>@em.PhoneNumber</td>
                <td>@em.EmploymentStartDate</td>
                <td>@em.Salary lei</td>
            </tr>
        }
    </tbody>
</table>